#PSEUDOCODE
START
STORE "Number" with any number
STORE "n" with any number
WHILE "Number" <= "n"
   CALCULATE "Number" mod 3
   SET "mod 3" with the result of calculation
   CALCULATE "Number" mod 5
   SET "mod 5" with the result of calculation
   IF "mod 3" equal to 0 AND "mod 5" not equal to 0
      DISPLAY "Fizz"
   ELIF "mod 3" not equal to 0 AND "mod 5" equal to 0
      DISPLAY "Buzz"
   ELIF "mod 3" AND "mod 5" equal to 0
      DISPLAY "FizzBuzz"
   ELSE 
      DISPLAY "Number"
END

#KASUS 1
START
STORE Number = 1
STORE n = 20
WHILE 1 <= 20 -> kondisi WHILE terpenuhi
   CALCULATE 1 mod 3 
   SET mod 3 = not 0 -> 1 tidak hasis dibagi 3
   CALCULATE 1 mod 5
   SET mod 5 = not 0 -> 1 tidak habis dibagi 5
   DISPLAY 1 -> kondisi ELSE yang terpenuhi
END

#KASUS 2
START
